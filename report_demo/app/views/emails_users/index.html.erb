<p id="notice"><%= notice %></p>

<h1 style="color:blue">Emails Users</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @emails_users.each do |emails_user| %>
      <tr>
        <td><%= emails_user.email_id %></td>
        <td><%= emails_user.user_id %></td>
        <td><%= link_to 'Show', emails_user %></td>
        <td><%= link_to 'Edit', edit_emails_user_path(emails_user) %></td>
        <td><%= link_to 'Destroy', emails_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Emails User', new_emails_user_path %>
